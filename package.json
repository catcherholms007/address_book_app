{
  "name": "address_book_app",
  "version": "0.1.0",
  "homepage": "/address_book_app/",
  "private": true,
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "firebase": "^5.6.0",
    "lodash": "^4.17.11",
    "mobx": "^5.6.0",
    "mobx-react": "^5.4.2",
    "normalize.css": "^8.0.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
