{
//  "rules": {
//    "semi": ["error", "always"],
//    "quotes": ["error", "double"]
//  },
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "quotes": ["error", "single"],
    "no-plusplus": "off",
    "linebreak-style": "off",
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": ["error", { "skipBlankLines": true }]
  },
  "env": {
    "browser": true
  }
}
